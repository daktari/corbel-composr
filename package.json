  ],
  "homepage": "http://opensource.bq.com/corbel-composer",
  "repository": {
    "type": "git",
    "url": "git@github.com:bq/corbel-composer.git"
  },
  "engines": {
    "node": "0.12.2",
    "npm": "~2.1.9"
  },
  "scripts": {
    "test:debug": "node-debug $(which grunt) test",
    "start": "node node_modules/pm2/bin/pm2 start bin/composer -i max",
    "stop": "node node_modules/pm2/bin/pm2 stop composer",
    "restart": "node node_modules/pm2/bin/pm2 restart composer",
    "debug": "node-debug bin/composer",
    "clean": "rm -rf node_modules",
    "logs": "node node_modules/pm2/bin/pm2 logs",
    "test": "grunt test"
  },
